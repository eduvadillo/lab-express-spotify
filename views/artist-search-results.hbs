<div class="backgroundArtist">
  <div>
    <h1>Los artistas que coinciden con {{artist}}tu busqueda son: </h1></div>
  <div class="general">
    {{#each dataArtist}}
      <div class="card" style="width: 22rem;">
        <img src="{{images.0.url}}" class="card-img-top" alt="..." />
        <div class="card-body">
          <h5 class="card-title">{{name}}</h5>
          <p class="card-text">{{genres}}</p>
          <a href="/albums/{{id}}" class="btn btn-primary">View Albums</a>
        </div>
      </div>
    {{/each}}
  </div>
  <a
    class="btn btn-primary BtnBack"
    href="javascript:history.back()"
    role="button"
  >Volver a elegir artista</a>

</div>